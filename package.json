	"scripts": {
		"dev:webpack": "node scripts/start.js",
		"dev:esbuild": "node scripts/esbuild.dev.js",
		"dev:run:electron": "electron .",
		"build": "node scripts/build.js",
		"esbuild": "node scripts/es_build.js",
		"test": "node scripts/test.js",
		"pack": "electron-packager ./ Gallery --platform=win32 --arch=x64 --icon=public/photo.ico --out=~/ --asar --app-version=0.0.1 --overwrite",
		"package": "npm run build && npm run pack",
		"package:esbuild": "npm run esbuild && npm run pack",
		"package:bishe": "npm run esbuild && electron-packager ./ Gallery --platform=win32 --arch=x64 --icon=public/photo.ico --out=app/ --asar --app-version=0.0.1 --overwrite",
		"package:electron-builder": "npm run esbuild && electron-builder --dir"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"browser": {
		"crypto": false
	},
	"devDependencies": {
		"@babel/core": "^7.17.8",
